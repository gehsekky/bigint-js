{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "func-names": 0,
        "no-mixed-operators": 0,
        "arrow-body-style": 0,
        "no-param-reassign": 0,
        "no-continue": 0,
        "no-extend-native": 0
    }
}
